{
  "name": "a2-firebase-demo",
  "version": "0.6.0",
  "description": "angular2-firebase Demo",
  "main": "app.js",
  "scripts": {
    "postinstall": "typings install && concat-cli -f node_modules/es6-shim/es6-shim.min.js node_modules/zone.js/dist/zone.js node_modules/reflect-metadata/Reflect.js -o www/poly.js",
    "build": "webpack -p --colors --display-error-details --display-cached",
